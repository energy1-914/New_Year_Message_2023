ormData: \", [...formData]); // [['title', 'dd'], ['content', 'dd']]\n\n      Api.post(body)\n        .then(response => console.log(\"성공. post결과 : \", response))\n        .catch(error => console.log(\"에러 : \", error));\n    };\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"48acf1ce996f93cd406d87333ece039a","cacheData":{"env":{}}}